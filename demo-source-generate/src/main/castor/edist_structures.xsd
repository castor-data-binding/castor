	<xsd:simpleType name="UTM_AppListType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for a UTM Parameter Role List.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Campaign">
				<xsd:annotation>
					<xsd:documentation source="description" xml:lang="en">Campaign content group.
Ex: utm_campaign=20percentpromocode
</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Source">
				<xsd:annotation>
					<xsd:documentation source="description" xml:lang="en">Referrer website idSource.
Example: utm_source=FareCompare
</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Medium">
				<xsd:annotation>
					<xsd:documentation source="description" xml:lang="en">Link medium type.
Example: utm_medium=socialmedia
</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Other"/>
		</xsd:restriction>
