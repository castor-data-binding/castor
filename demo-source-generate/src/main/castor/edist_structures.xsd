			<xsd:documentation source="description" xml:lang="en">A data type for Language Code Application.

Examples: Display, Requested, Spoken, Written, Other</xsd:documentation>
		</xsd:annotation>
		<xsd:union memberTypes="ApplicationSimpleType UTM_AppListType"/>
	</xsd:simpleType>
	<!--<< PASSENGER TYPE >>-->
	<xsd:simpleType name="PaxSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Passenger Type Code (PTC) encoding constraint: IATA (three character) Passenger Type Codes</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:length value="3"/>
			<xsd:pattern value="[A-Z]{3}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!--<< PAYMENT >>-->
	<xsd:simpleType name="ApprovalCodeSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for a Payment Card Approval Code constraint.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[0-9a-zA-Z]{1,8}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PayCardTypeSimpleType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">A data type for a Credit Card Type constraint.</xsd:documentation>
		</xsd:annotation>
		<xsd:union>
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:pattern value="[0-9a-zA-Z]{1,6}"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:union>
	</xsd:simpleType>
	<xsd:simpleType name="PayCardCodeSimpleType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">A data type for a Credit Card Code constraint.</xsd:documentation>
		</xsd:annotation>
		<xsd:union>
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:pattern value="[A-Z]{2}"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:union>
	</xsd:simpleType>
	<xsd:simpleType name="PayCardNmbrSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for a Credit Card Number constraint.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[0-9]{1,19}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PayCardCVV_SimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for a Payment Card Series Code constraint.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[0-9]{1,8}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PayCardDateSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for a Payment Card Date constraint.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="(0[1-9]|1[0-2])[0-9][0-9]"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PayFormListType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Baggage Applicable To contextual list.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="BankAccount"/>
			<xsd:enumeration value="Cash"/>
			<xsd:enumeration value="CreditCard"/>
			<xsd:enumeration value="DebitCard"/>
			<xsd:enumeration value="DirectBill"/>
			<xsd:enumeration value="LoyaltyRedemption"/>
			<xsd:enumeration value="MiscChargeOrder"/>
			<xsd:enumeration value="PaymentCard"/>
			<xsd:enumeration value="Voucher"/>
			<xsd:enumeration value="Other"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PayFormSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Baggage Application.</xsd:documentation>
		</xsd:annotation>
		<xsd:union memberTypes="ApplicationSimpleType PayFormListType"/>
	</xsd:simpleType>
	<xsd:simpleType name="PaymentFormListType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">
        A data type for Accepted Payment Form contextual list.
      </xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="BankAccount"/>
			<xsd:enumeration value="Cash"/>
			<xsd:enumeration value="CreditCard"/>
			<xsd:enumeration value="DebitCard"/>
			<xsd:enumeration value="DirectBill"/>
			<xsd:enumeration value="LoyaltyRedemption"/>
			<xsd:enumeration value="MiscChargeOrder"/>
			<xsd:enumeration value="PaymentCard"/>
			<xsd:enumeration value="Voucher"/>
			<xsd:enumeration value="Other"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PaymentFormSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Accepted Payment Form list.</xsd:documentation>
		</xsd:annotation>
		<xsd:union memberTypes="ApplicationSimpleType PaymentFormListType"/>
	</xsd:simpleType>
	<xsd:simpleType name="PaymentStatusListType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">
        A data type for Payment Status contextual list. Examples: Canceled, Complete, Failed, MoreInformation, Partial
      </xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Canceled"/>
			<xsd:enumeration value="Complete"/>
			<xsd:enumeration value="Failed"/>
			<xsd:enumeration value="MoreInformation"/>
			<xsd:enumeration value="Partial"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PaymentStatusSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">
        A data type for Payment Status. Examples: Canceled, Complete, Failed, MoreInformation, Partial
      </xsd:documentation>
		</xsd:annotation>
		<xsd:union memberTypes="ApplicationSimpleType PaymentStatusListType"/>
	</xsd:simpleType>
	<xsd:simpleType name="MaskedCardNmbrSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for a Masked Payment Card Number constraint.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[0-9a-zA-Z]{1,19}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!--<< PENALTY >>-->
	<xsd:simpleType name="PenaltyAppListType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Offer Penalty Amount Application list constraint.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="MaximumPenaltyAmount"/>
			<xsd:enumeration value="MinimumPenaltyAmount"/>
			<xsd:enumeration value="Other"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PenaltyAppSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Offer Penalty Amount Application.</xsd:documentation>
		</xsd:annotation>
		<xsd:union memberTypes="ApplicationSimpleType PenaltyAppListType"/>
	</xsd:simpleType>
	<xsd:simpleType name="PenaltyListType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Offer Penalty Type list constraint.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Cancellation"/>
			<xsd:enumeration value="Change"/>
			<xsd:enumeration value="NoShow"/>
			<xsd:enumeration value="Upgrade"/>
			<xsd:enumeration value="Other"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PenaltySimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Offer Penalty Type.</xsd:documentation>
		</xsd:annotation>
		<xsd:union memberTypes="ApplicationSimpleType PenaltyListType"/>
	</xsd:simpleType>
	<!--<< PERCENTAGE >>-->
	<xsd:simpleType name="WholePercentageSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for  Whole Percentage encoding constraint.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:decimal">
			<xsd:minInclusive value="0.01"/>
			<xsd:maxInclusive value="100"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!--<< PREFERENCE >>-->
	<xsd:simpleType name="PreferencesLevelListType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Preference Type list constraint.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Preferred"/>
			<xsd:enumeration value="Required"/>
			<xsd:enumeration value="Exclude"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PreferencesLevelSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Language Code Application.

Examples: Display, Requested, Spoken, Written, Other</xsd:documentation>
		</xsd:annotation>
		<xsd:union memberTypes="ApplicationSimpleType PreferencesLevelListType"/>
	</xsd:simpleType>
	<!--<< PROPER NAME >>-->
	<xsd:simpleType name="ProperNameSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Proper Name size constraint.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="50"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PropertyNameSimpleType_">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Property Name size constraint.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="50"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!--<< PROXIMITY >>-->
	<xsd:simpleType name="ProximityAppListType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Proximity Data Application list constraint.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="ActualProximity"/>
			<xsd:enumeration value="SearchRadius"/>
			<xsd:enumeration value="Other"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ProximityAppSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Proximity Data Application.</xsd:documentation>
		</xsd:annotation>
		<xsd:union memberTypes="ApplicationSimpleType ProximityAppListType"/>
	</xsd:simpleType>
	<!--<< PSEUDO CITY >>-->
	<xsd:simpleType name="PseudoCitySimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for  Pseudo City Code encoding constraint. Example: AAA2</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="1"/>
			<xsd:maxLength value="16"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!--<< REDEMPTION PRICING >>-->
	<xsd:simpleType name="RedempUnitListType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for a Redemption Unit List.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Miles"/>
			<xsd:enumeration value="Points"/>
			<xsd:enumeration value="Other"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="RedemptionUnitSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Language Code Application.

Examples: Display, Requested, Spoken, Written, Other</xsd:documentation>
		</xsd:annotation>
		<xsd:union memberTypes="ApplicationSimpleType RedempUnitListType"/>
	</xsd:simpleType>
	<!--<< RULES >>-->
	<xsd:simpleType name="RuleStatusListType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Rule Status contextual list.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="ActionRequired"/>
			<xsd:enumeration value="RuleExecuted"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="RuleStatusSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Language Code Application.

Examples: Display, Requested, Spoken, Written, Other</xsd:documentation>
		</xsd:annotation>
		<xsd:union memberTypes="ApplicationSimpleType RuleStatusListType"/>
	</xsd:simpleType>
	<!--<< SPECIAL SERVICE >>-->
	<xsd:simpleType name="SpecialSrvcSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Special Service or Request Code constraint.</xsd:documentation>
		</xsd:annotation>
		<xsd:union memberTypes="SSR_SimpleType CodesetValueSimpleType"/>
	</xsd:simpleType>
	<xsd:simpleType name="SSR_SimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Special Service Request Code encoding constraint: IATA
Examples:
EXST (EXTRA SEAT REQUIRED)
INFT (INFANT ACCOMPANYING ADULT PASSENGER)
PETC (PASSENGER TRAVELING WITH PET IN CABIN)
UMNR (UNACCOMPANIED MINOR)</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[A-Z]{4}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!--<< STANDARDS >>-->
	<xsd:simpleType name="IATA_NbrSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for IATA Company Number encoding constraint. Example: 98417900</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[A-Z0-9]{7,8}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ICAO_LocSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for ICAO Location Code encoding constraint. Example: K (CONTIGUOUS UNITED STATES)</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="1"/>
			<xsd:maxLength value="2"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!--<< TARGET SYSTEM >>-->
	<xsd:simpleType name="TargetSystemListType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for a Message Target System Type constrained list.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Production">
				<xsd:annotation>
					<xsd:documentation source="description" xml:lang="en">K = KILOMETERS</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Staging">
				<xsd:annotation>
					<xsd:documentation source="description" xml:lang="en">M = MILES</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Test"/>
			<xsd:enumeration value="Other"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TargetSystemSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Language Code Application.

Examples: Display, Requested, Spoken, Written, Other</xsd:documentation>
		</xsd:annotation>
		<xsd:union memberTypes="ApplicationSimpleType TargetSystemListType"/>
	</xsd:simpleType>
	<xsd:simpleType name="TraceIP_SimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for IP Address encoding constraint.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="((((25[0-5])|(2[0-4][0-9])|([01]?[0-9]?[0-9]))\.){3}((25[0-5])|(2[0-4][0-9])|([01]?[0-9]?[0-9])))(\-((((25[0-5])|(2[0-4][0-9])|([01]?[0-9]?[0-9]))\.){3}((25[0-5])|(2[0-4][0-9])|([01]?[0-9]?[0-9]))))?"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!--<< TICKET >>-->
	<xsd:simpleType name="TicketDesigAppListType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Ticket Designator Application list constraint.

Examples: Requested, Ticketed, Other</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Requested"/>
			<xsd:enumeration value="Ticketed"/>
			<xsd:enumeration value="Other"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TicketDesigAppSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Language Code Application.

Examples: Display, Requested, Spoken, Written, Other</xsd:documentation>
		</xsd:annotation>
		<xsd:union memberTypes="ApplicationSimpleType TicketDesigAppListType"/>
	</xsd:simpleType>
	<xsd:simpleType name="TicketNumberSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">
        A data type for Ticket Number Constraint. Examples: String, Length 1 to 16.
      </xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="1"/>
			<xsd:maxLength value="16"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- << TIME LIMIT >> -->
	<xsd:simpleType name="TimeLimitTypeListType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">
        A data type for NDC Time Limit Type list constraint. Examples: Bilateral, Deposit, InventoryGuarantee, Naming, Offer, Payment, PriceGuarantee
      </xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Bilateral"/>
			<xsd:enumeration value="Deposit"/>
			<xsd:enumeration value="InventoryGuarantee"/>
			<xsd:enumeration value="Naming"/>
			<xsd:enumeration value="Offer"/>
			<xsd:enumeration value="Payment"/>
			<xsd:enumeration value="PriceGuarantee"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TimeLimitTypeSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">
        A data type for NDC Time Limit Type. Examples: Bilateral, Deposit, InventoryGuarantee, Naming, Offer, Payment, PriceGuarantee
      </xsd:documentation>
		</xsd:annotation>
		<xsd:union memberTypes="ApplicationSimpleType TimeLimitTypeListType"/>
	</xsd:simpleType>
	<!--<< TRIP >>-->
	<xsd:simpleType name="TravelerGenderSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Traveler Gender list constraint.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Male"/>
			<xsd:enumeration value="Female"/>
			<xsd:enumeration value="Unknown"/>
			<xsd:enumeration value="Other"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TripPurposeListType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Trip Purpose contextual list.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Business"/>
			<xsd:enumeration value="Leisure"/>
			<xsd:enumeration value="BusinessAndLeisure"/>
			<xsd:enumeration value="Other"/>
			<xsd:enumeration value="Unknown"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TripPurposeSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Partner Company Type.</xsd:documentation>
		</xsd:annotation>
		<xsd:union memberTypes="ApplicationSimpleType TripPurposeListType"/>
	</xsd:simpleType>
	<!-- << TRANSACTION PROCESSING >> -->
	<xsd:simpleType name="TrxProcessingListType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">
        A data type for NDC Transaction Processing Status contextual list. Example: Complete, InComplete, Unknown
      </xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Complete"/>
			<xsd:enumeration value="InComplete"/>
			<xsd:enumeration value="Unknown"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TrxProcessingSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">
        A data type for NDC Transaction Processing Status. Example: Complete, InComplete, Unknown
      </xsd:documentation>
		</xsd:annotation>
		<xsd:union memberTypes="ApplicationSimpleType TrxProcessingListType"/>
	</xsd:simpleType>
	<!--<< UNIQUE IDENTIFIER >>-->
	<xsd:simpleType name="IdentifierSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Service, Offer, Order and Order Item ID size constraint.</xsd:documentation>
			<xsd:documentation source="Rules" xml:lang="en">Encoding: Alphanumeric
Min Length: 1
Max Length: 45

This unique ID is limited to a maximum length of 45 characaters to adhere to NDC Interline Task Force requirements. Note that this 45 character unique ID and an associated "idSource" attribute may be specified to gauarantee uniqueness.</xsd:documentation>
			<xsd:documentation source="Example" xml:lang="en">Example: 93E0CA5D-1C3A-4322-82E9-170BFAB02123</xsd:documentation>
			<xsd:documentation source="Guidelines" xml:lang="en">Notes: This ID is used in conjunction with an airline designator code.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="45"/>
			<xsd:minLength value="1"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="UniqueStringID_SimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for a Unique String Identifier constraint.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="1"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!--<< UNITS OF MEASURE >>-->
	<xsd:simpleType name="SizeUnitSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Size Values list constraint. Examples: Centimeter, Inch</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Centimeter"/>
			<xsd:enumeration value="Inch"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="WeightUnitSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Weight Unit of Measure list constraint. Examples: Kilogram, Pound</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Kilogram"/>
			<xsd:enumeration value="Pound"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!--<< ZONE >>-->
	<xsd:simpleType name="ZoneCodeSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Geographical Zone Code constraint.

Unique Geographical Zone Code.  Example: 001

Notes: Use of IATA Padis Codeset - List for data element GZO (Geographical Zone) is recommended. Any string value up to 15 characters in length is accepted.</xsd:documentation>
		</xsd:annotation>
		<xsd:union memberTypes="SSR_SimpleType CodesetValueSimpleType"/>
	</xsd:simpleType>
	<!-- << POLICY >> -->
	<xsd:simpleType name="PolicyTypeSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">
        A data type for Policy Type list constraint. Examples: Privacy_PII, Payment_PCI, Credentialing
      </xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Privacy_PII">
				<xsd:annotation>
					<xsd:documentation>
            Personnally Identifting Information Privacy Policy.
          </xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Payment_PCI"/>
			<xsd:enumeration value="Credentialing"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PolicyTypeListType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">
        A data type for Policy Type. Examples: Privacy_PII, Payment_PCI, Credentialing
      </xsd:documentation>
		</xsd:annotation>
		<xsd:union memberTypes="ApplicationSimpleType PolicyTypeSimpleType"/>
	</xsd:simpleType>
	<!-- << RESHOPPING >> -->
	<xsd:simpleType name="AlphaNumericStringLength1to2">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Used for Alpha-Numeric Strings, length 1 to 2</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[0-9a-zA-Z]{1,2}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="AlphaNumericStringLength1to3">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Used for Alpha-Numeric Strings, length 1 to 3</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[0-9a-zA-Z]{1,3}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="AlphaLength3">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Used for Alphabetic Strings, length exactly 3</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[a-zA-Z]{3}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="AirlineType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Type definition for 3 or 2 letter airline codes</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="2"/>
			<xsd:maxLength value="3"/>
			<xsd:pattern value="([A-Z]{3}|[A-Z]{2})|([0-9][A-Z])|([A-Z][0-9])"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="AlphaNumericStringLength1to71">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Used for Alpha-Numeric Strings, length 1 to 71</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[0-9a-zA-Z]{1,71}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="AlphaNumericStringLength1to4">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Used forAlpha-Numeric Strings, length 1 to 4.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[0-9a-zA-Z]{1,4}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Numeric0to99">
		<xsd:annotation>
			<xsd:documentation>Use for numeric values, from 0 to 99 inclusive.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:integer">
			<xsd:minInclusive value="0"/>
			<xsd:maxInclusive value="99"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="NumericStringLength1to19">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Used for Numeric Strings, length 1 to 19.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[0-9]{1,19}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="OrderItemActionSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">
        A data type for an Order Action list constraint. Examples: Cancel, Create, Divide, Delete, Reissue, Reprice, Update
      </xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Cancel"/>
			<xsd:enumeration value="Create"/>
			<xsd:enumeration value="Delete"/>
			<xsd:enumeration value="Divide"/>
			<xsd:enumeration value="Reissue"/>
			<xsd:enumeration value="Reprice"/>
			<xsd:enumeration value="Update"/>
		</xsd:restriction>
	</xsd:simpleType>
