	<xsd:simpleType name="SecureActionListType">
		<xsd:annotation>
			<xsd:documentation>
        A data type for 3DS Action Type contextual list. Examples: CheckEnrollment CompleteAuthentication DoAuthentication ValidateAddress ValidateCVC
      </xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="EnrollmentCheckRequest"/>
			<xsd:enumeration value="EnrollmentCheckResponse"/>
			<xsd:enumeration value="CardAuthenticationRequest"/>
			<xsd:enumeration value="CardAuthenticationResponse"/>
