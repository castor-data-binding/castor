			<xsd:documentation source="description" xml:lang="en">A data type for Summary Frequent Traveler Program.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="FQTV_ProgramCoreType">
				<xsd:sequence>
					<xsd:element name="URL" minOccurs="0">
