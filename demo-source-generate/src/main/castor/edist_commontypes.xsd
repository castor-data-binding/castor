					<xsd:element name="Status" type="OfferStatusType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Offer Status. Example: NN (Requested)</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
