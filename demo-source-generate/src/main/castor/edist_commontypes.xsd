									<xsd:element ref="ParticipatingProgram">
										<xsd:annotation>
											<xsd:documentation>Identifies a particular affiliated FQTV program in an Alliance in view of an FQTV program details.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
		<xsd:attributeGroup ref="ObjAssociationAttrGroup"/>
	</xsd:complexType>
	<xsd:complexType name="AllianceAirlinePartyType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">FQTV ALLIANCE- AIRLINE PARTICIPANTS representation.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="AirlineID">
				<xsd:annotation>
					<xsd:documentation>A two-character alpha-numeric code assigned according to the IATA Resolution 762 or a three-letter airline designator assigned by ICAO.  Refer to IATA Airline Coding Directory</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ProgramName" type="ProperNameSimpleType" minOccurs="0">
