			<xsd:documentation source="description" xml:lang="en">FQTV ALLIANCE- PROGRAM PARTICIPANTS representation.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="ProgramName" type="ProperNameSimpleType">
				<xsd:annotation>
					<xsd:documentation source="description" xml:lang="en">Airline-sponsored FQTV Program name.  Example: Miles and More</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="AirlineID" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A two-character alpha-numeric code assigned according to the IATA Resolution 762 or a three-letter airline designator assigned by ICAO.  Refer to IATA Airline Coding Directory</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attributeGroup ref="ObjAssociationAttrGroup"/>
	</xsd:complexType>
	<xsd:element name="ParticipatingProgram" type="AllianceProgramType">
