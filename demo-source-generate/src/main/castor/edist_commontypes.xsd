	<xsd:complexType name="FlightMetadataType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">FLIGHT Metadata definition.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="MetadataObjectBaseType">
				<xsd:sequence>
					<xsd:element name="ActionCode" type="CodesetValueSimpleType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Reservation system action code. Example: HK</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="BindingKey" type="UniqueStringID_SimpleType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">A unique flight segment reference that may be used when binding request/ response messages.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="FlightStatus" minOccurs="0"/>
					<xsd:element ref="Frequency" minOccurs="0"/>
					<xsd:element ref="InstantPurchase" minOccurs="0"/>
					<xsd:element name="Meals" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Meal(s) Served.</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="Meal" type="FlightMealsType" maxOccurs="unbounded">
									<xsd:annotation>
										<xsd:documentation source="description" xml:lang="en">Meal served code. Example: B (Breakfast)

Encoding Scheme: IATA Padis Codeset - List for data element 7161 - Special Service, Coded</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
					<xsd:element ref="OnTimePerformance" minOccurs="0"/>
					<xsd:element ref="Remarks" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
