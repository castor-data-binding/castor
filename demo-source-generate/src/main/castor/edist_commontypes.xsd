			<xsd:extension base="MetadataObjectBaseType">
				<xsd:sequence>
					<xsd:element ref="Attributes" minOccurs="0"/>
					<xsd:element name="FareTariff" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Tariff.
Example: PF17</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:simpleContent>
								<xsd:extension base="xsd:string">
									<xsd:attribute name="FareType" type="xsd:string" use="optional">
										<xsd:annotation>
											<xsd:documentation source="description" xml:lang="en">Fare Type.</xsd:documentation>
										</xsd:annotation>
									</xsd:attribute>
								</xsd:extension>
