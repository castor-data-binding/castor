	<xsd:complexType name="CodesetMetadataType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for PADIS/ CODESET Metadata.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="MetadataObjectBaseType">
				<xsd:sequence>
					<xsd:element name="Source" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Codeset Source.</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="OwnerID" minOccurs="0">
									<xsd:annotation>
										<xsd:documentation source="description" xml:lang="en">Owner Entity ID. Example: BA</xsd:documentation>
									</xsd:annotation>
									<xsd:complexType>
										<xsd:simpleContent>
											<xsd:extension base="xsd:string">
												<xsd:attribute name="Name" use="optional">
													<xsd:annotation>
														<xsd:documentation source="description" xml:lang="en">Owner Name. Example: British Airways</xsd:documentation>
													</xsd:annotation>
													<xsd:simpleType>
														<xsd:restriction base="ProperNameSimpleType"/>
													</xsd:simpleType>
												</xsd:attribute>
											</xsd:extension>
										</xsd:simpleContent>
									</xsd:complexType>
								</xsd:element>
								<xsd:element name="File" type="ShortDescSimpleType" minOccurs="0">
									<xsd:annotation>
										<xsd:documentation source="description" xml:lang="en">Codeset file or resource name. Example: PADIS Codeset</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="Version" type="CodesetValueSimpleType" minOccurs="0">
									<xsd:annotation>
										<xsd:documentation source="description" xml:lang="en">Codeset version. Example: 14</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="OtherLanguage" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Other (alternate) Language Encoding and Code Description.</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element ref="LanguageCode"/>
								<xsd:element name="Description" type="DescriptionSimpleType" minOccurs="0">
									<xsd:annotation>
										<xsd:documentation source="description" xml:lang="en">Codeset table or list name.  Example: Data Element 9972-Originator Type</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
