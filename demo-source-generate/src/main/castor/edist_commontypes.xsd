	</xsd:simpleType>
	<xsd:simpleType name="FareBasisAppSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Fare Basis Application.</xsd:documentation>
		</xsd:annotation>
		<xsd:union memberTypes="ApplicationSimpleType FareBasisListType"/>
	</xsd:simpleType>
