	<xsd:element name="ItemStatus" type="CodesetType">
		<xsd:annotation>
			<xsd:documentation>
        Current status of the Order Item. Example: OK (Confirmed) Encoding Scheme: IATA Padis Codeset - List for data element [4405] Status, coded
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
