	<xsd:complexType name="AddressMetadataType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for ADDRESS Metadata.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="MetadataObjectBaseType">
				<xsd:sequence>
					<xsd:element ref="Position" minOccurs="0"/>
					<xsd:element name="AddressType" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>type of address eg Billing, Business, Home</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="AddressFields" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>list of address fields required by vendor</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="FieldName" maxOccurs="unbounded">
									<xsd:complexType>
										<xsd:simpleContent>
											<xsd:extension base="xsd:string">
												<xsd:attribute name="Mandatory" type="xsd:boolean" use="required">
													<xsd:annotation>
														<xsd:documentation>If this field is required then TRUE, otherwise FALSE.</xsd:documentation>
													</xsd:annotation>
												</xsd:attribute>
											</xsd:extension>
										</xsd:simpleContent>
									</xsd:complexType>
								</xsd:element>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="AircraftMetadataType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for AIRCRAFT Metadata.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
