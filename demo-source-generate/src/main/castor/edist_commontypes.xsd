			<xsd:extension base="LocationProximityType">
				<xsd:sequence>
					<xsd:element ref="CountryCode">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Country Code. Examples: US, FR

Encoding Scheme: ISO 3166-1 (two character) Country Code</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
