			<xsd:documentation source="description" xml:lang="en">Eligible Class of Service Upgrade information.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ServiceCommissionInfo" type="InstrCommissionType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">Information relating to the commission amount claimable by a Travel Agent for services on behalf of an airline.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ServiceFreeFormTextInstruction" type="InstrRemarkType">
