						</xsd:complexType>
					</xsd:element>
					<xsd:element name="Name" type="ProperNameSimpleType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Program Name.  Example: Miles and More</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
