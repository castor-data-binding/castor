			<xsd:documentation source="description" xml:lang="en">A data type for Detail Frequent Traveler Program.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="FQTV_ProgramSummaryType">
				<xsd:sequence>
					<xsd:element name="Alliances" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">FQTV Program Alliances(s).</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="Alliance" type="FQTV_AllianceType" maxOccurs="unbounded">
									<xsd:annotation>
										<xsd:documentation source="description" xml:lang="en">FQTV Alliance detail.</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:element name="TravelerFQTV_Information" type="TravelerFQTV_Type">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">Traveler Frequent Flyer (FQTV) Account Information.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="TravelerFQTV_Type">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">Traveler FQTV ACCOUNT definition.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="AirlineID">
				<xsd:annotation>
					<xsd:documentation source="description" xml:lang="en">FQTV Program Sponsoring Airline.  Example: BA

Encoding Scheme: IATA/ A4A (two or three character) Airline Designator Code</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Account" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation source="description" xml:lang="en">Traveler FQTV Account information.</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="SignInID" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation source="description" xml:lang="en">Traveler FQTV program sign in (or user) ID.</xsd:documentation>
							</xsd:annotation>
							<xsd:complexType>
								<xsd:simpleContent>
									<xsd:extension base="xsd:string">
										<xsd:attributeGroup ref="ObjAssociationAttrGroup"/>
									</xsd:extension>
								</xsd:simpleContent>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="Number">
							<xsd:annotation>
								<xsd:documentation source="description" xml:lang="en">FQTV account number.
Example: FQTV123456</xsd:documentation>
							</xsd:annotation>
							<xsd:complexType>
								<xsd:simpleContent>
									<xsd:extension base="xsd:string">
										<xsd:attributeGroup ref="ObjAssociationAttrGroup"/>
									</xsd:extension>
								</xsd:simpleContent>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="FQTVStatus" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Frequent flyer tier status. Example: "Altitude Elite 100K"</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
					<xsd:attributeGroup ref="ObjAssociationAttrGroup"/>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="ProgramID" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The Program ID will be used to specify Frequent Flyer Program. This will be the name of the program</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attributeGroup ref="ObjAssociationAttrGroup"/>
	</xsd:complexType>
	<!--<< Alliance >>-->
	<xsd:complexType name="FQTV_AllianceType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">FQTV PROGRAM ALLIANCE representation.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Code">
				<xsd:annotation>
					<xsd:documentation source="description" xml:lang="en">Used to specify the alliance CodeSet. Refer to IATA PADIS Code List for data element 9906. Used in conjunction with Name (Alliance Name) and Participants (Alliance Particiaption information)</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:complexContent>
						<xsd:extension base="UniqueIDContextType"/>
					</xsd:complexContent>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="URL" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation source="description" xml:lang="en">Alliance information URL.
Example: http://www.oneworld.com/home</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="xsd:anyURI"/>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="Name" type="ProperNameSimpleType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation source="description" xml:lang="en">Alliance name. Example: One World Alliance</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Participants" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation source="description" xml:lang="en">"Alliance Participation information.  Used in conjunction with Code (Unique Alliance code or ID-OWA
 and Name (Alliance Name-One World Alliance)."</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="Participant" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation source="description" xml:lang="en">Participant detail.</xsd:documentation>
							</xsd:annotation>
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element ref="ParticipatingAirline">
