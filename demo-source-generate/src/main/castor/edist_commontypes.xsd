	<xsd:simpleType name="PriceClassAssocListItemType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Price Class Associations.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Disclosure"/>
			<xsd:enumeration value="MediaGroup"/>
			<xsd:enumeration value="MediaItem"/>
			<xsd:enumeration value="Penalty"/>
			<xsd:enumeration value="Service"/>
			<xsd:enumeration value="ServiceBundle"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PriceClassAssocListType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type Price Class Association Type Name.</xsd:documentation>
		</xsd:annotation>
		<xsd:union memberTypes="ApplicationSimpleType PriceClassAssocListItemType"/>
	</xsd:simpleType>
	<!--====================================================
===  N A M E D   A S S O C I A T I O N S
====================================================-->
	<xsd:complexType name="PriceClassAssocType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">Price Class Associations.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Association" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation source="description" xml:lang="en">Price Class Association Detail.</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="Type" type="PriceClassAssocListType">
							<xsd:annotation>
								<xsd:documentation source="description" xml:lang="en">Named association type. Examples: ServiceBundle, MediaGroup</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="ReferenceValue" type="xsd:IDREF">
							<xsd:annotation>
								<xsd:documentation source="description" xml:lang="en">Associated object instance value. Example: mediagrp1</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element ref="ServiceReference" minOccurs="0" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!--===================================================
===  C O R E  D E F I N I T I O N S
===================================================-->
	<xsd:complexType name="PriceClassType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">PRICE CLASS definition.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Name" type="ProperNameSimpleType">
				<xsd:annotation>
					<xsd:documentation source="description" xml:lang="en">Price class name. Example: SUPERSAVER</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Code" type="CodesetValueSimpleType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation source="description" xml:lang="en">Price class code. Example: 1, ABC</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Descriptions" type="Descriptions" minOccurs="0"/>
			<xsd:element ref="FareBasisCode" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation source="description" xml:lang="en">Fare Basis Code with optional application. Example: Y26</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="ClassOfService" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="Associations" type="PriceClassAssocType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation source="description" xml:lang="en">Price Class Associations.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DisplayOrder" type="CodesetValueSimpleType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation source="description" xml:lang="en">The Airline requested default display order (in a client application) for this price class. Example: 2</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attributeGroup ref="ObjAssociationAttrGroup"/>
		<xsd:attribute name="ObjectKey" type="xsd:ID" use="optional">
			<xsd:annotation>
				<xsd:documentation source="description" xml:lang="en">Object globally unique Instance ID. Example: obj1</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
