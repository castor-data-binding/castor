<?xml version="1.0"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN" 
    "http://castor.org/mapping.dtd">
<mapping>

   <class name="org.castor.cpa.test.test85.Product" identity="id">
        <map-to table="test85_enum_prod"/>
        <field name="id" type="integer">
            <sql name="id" type="integer" />
        </field>
        <field name="name" type="string" required="true">
            <sql name="name" type="varchar" />
        </field>
        <field name="kind" type="org.castor.cpa.test.test85.KindEnum" required="true">
            <sql name="kind" type="varchar" />
        </field>
    </class>

</mapping>
