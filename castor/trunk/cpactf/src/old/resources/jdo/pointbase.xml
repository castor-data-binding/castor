<?xml version="1.0" ?>
<!DOCTYPE jdo-conf PUBLIC "-//EXOLAB/Castor JDO Configuration DTD Version 1.0//EN"
                           "http://castor.org/jdo-conf.dtd">

<jdo-conf>
	<database name="pointbase" engine="pointbase">
		<driver url="jdbc:pointbase://localhost:9092/castor" class-name="com.pointbase.jdbc.jdbcUniversalDriver">
				<param name="user" value="pbpublic" />
				<param name="password" value="pbpublic" />
		</driver>
		<mapping href="jdo/mapping.xml" />
	</database>
	<transaction-demarcation mode="local" />
</jdo-conf>