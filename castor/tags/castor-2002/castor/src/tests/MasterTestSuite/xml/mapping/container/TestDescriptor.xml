<?xml version='1.0'?>
<!--Example of a testDescriptor-->
<TestDescriptor>

	<Name>Object container test</Name>

	<Comment>
        This test check the object container code. It will check:
                - basic container for elements and attributes
                - container object in container object
    </Comment>

	<Category>basic capability</Category>

	<MarshallingTest>

		<Root_Object random="false" dump="true">A</Root_Object>
        <Mapping_File>mapping.xml</Mapping_File>

		<UnitTestCase>
            <Name>Simple Test</Name>
			<Input>input.xml</Input>
			<ObjectBuilder>Builder</ObjectBuilder>
			<Output>output.xml</Output>
		</UnitTestCase>

	</MarshallingTest>
</TestDescriptor>