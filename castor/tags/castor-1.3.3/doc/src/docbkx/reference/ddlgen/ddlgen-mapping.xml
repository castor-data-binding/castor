    <section>
    	<title>JDBC Types not supported by Castor</title>
        
		<para>
			The following JDBC types are not supported by Castor yet. 
		</para>

		<itemizedlist>
			<listitem>ARRAY</listitem>
			<listitem>DISTINCT</listitem>
			<listitem>REF</listitem>
			<listitem>STRUCT</listitem>
		</itemizedlist>
		
