<?xml version="1.0"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
                           "http://castor.org/mapping.dtd">
<mapping>
	<class name="org.exolab.castor.jdo.engine.Entity">
		<description>Entity</description>
		<map-to table="entity" />
		<field name="id" type="integer" identity="true">
			<sql name="id" type="integer" />
		</field>
		<field name="item" type="org.exolab.castor.jdo.engine.Item">
			<sql name="item_id" />
		</field>
	</class>
	<class name="org.exolab.castor.jdo.engine.Item">
		<description>Item</description>
		<map-to table="item" />
		<field name="id" type="integer" identity="true">
			<sql name="id" type="integer" />
		</field>
	</class>
</mapping>
