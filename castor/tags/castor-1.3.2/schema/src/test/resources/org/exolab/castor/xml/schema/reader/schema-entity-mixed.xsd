<?xml version="1.0" encoding="UTF-8" ?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	targetNamespace="http://castor.org/test/ase"
	xmlns:xyz="http://www.foo.xyz"
	xmlns:jdo="http://www.castor.org/binding/persistence"
	xmlns="http://castor.org/test/ase">

	<xs:annotation>
		<xs:documentation>
			This is a test XML Schema with appinfo annotations for
			Entity testcase.
		</xs:documentation>
	</xs:annotation>

	<xs:element name="book" type="bookType" />

	<!--  Description of a bookType -->
	<xs:complexType name="bookType">
		<xs:annotation>
			<xs:appinfo>
				<jdo:foo-content-with-sub-elements>
				    <jdo:table>
				        <jdo:foo/>
				    </jdo:table>
				</jdo:foo-content-with-sub-elements>
			</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="isbn" type="xs:string">
				<xs:annotation>
					<xs:appinfo>
						<jdo:foo-content />
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="title" type="xs:string">
				<xs:annotation>
					<xs:appinfo>
						<jdo:foo-content />
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
</xs:schema>