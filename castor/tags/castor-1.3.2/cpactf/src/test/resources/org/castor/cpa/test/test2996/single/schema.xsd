<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	targetNamespace="http://castor.org/cpa/test/test2996/single"
	xmlns:jdo="http://www.castor.org/binding/persistence"
	xmlns="http://castor.org/cpa/test/test2996/single">

	<xs:annotation>
		<xs:documentation>
			This is a test XML Schema with appinfo annotations for Entity testcase.
		</xs:documentation>
	</xs:annotation>

	<xs:element name="book" type="book" />

	<!--  Description of a bookType -->
	<xs:complexType name="book">
		<xs:annotation>
			<xs:appinfo>
				<jdo:table name="test2996_single_book">
					<jdo:primaryKey>
						<jdo:key>isbn</jdo:key>
					</jdo:primaryKey>
				</jdo:table>
			</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="isbn" type="xs:integer">
				<xs:annotation>
					<xs:appinfo>
						<jdo:column name="isbn" type="integer" />
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="title" type="xs:string">
				<xs:annotation>
					<xs:appinfo>
						<jdo:column name="title" type="varchar" />
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
</xs:schema>